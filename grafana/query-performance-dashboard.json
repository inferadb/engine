{
  "dashboard": {
    "title": "InferaDB - Query Performance",
    "tags": ["inferadb", "authorization", "performance"],
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Query Latency Percentiles by Operation",
        "type": "graph",
        "gridPos": { "h": 10, "w": 24, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum by (operation, le) (rate(inferadb_query_operation_duration_seconds_bucket[5m]))) * 1000",
            "legendFormat": "{{operation}} - p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.90, sum by (operation, le) (rate(inferadb_query_operation_duration_seconds_bucket[5m]))) * 1000",
            "legendFormat": "{{operation}} - p90",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum by (operation, le) (rate(inferadb_query_operation_duration_seconds_bucket[5m]))) * 1000",
            "legendFormat": "{{operation}} - p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          { "format": "ms", "label": "Latency" },
          { "format": "short" }
        ],
        "legend": {
          "show": true,
          "alignAsTable": true,
          "rightSide": false,
          "avg": true,
          "max": true,
          "current": true
        }
      },
      {
        "id": 2,
        "title": "Slowest Operations (p99 Latency)",
        "type": "table",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 10 },
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.99, sum by (operation, le) (rate(inferadb_query_operation_duration_seconds_bucket[5m]))) * 1000)",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": { "Time": true },
              "renameByName": { "operation": "Operation", "Value": "p99 Latency (ms)" }
            }
          }
        ]
      },
      {
        "id": 3,
        "title": "Average Query Duration by Operation",
        "type": "table",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 10 },
        "targets": [
          {
            "expr": "topk(10, avg by (operation) (rate(inferadb_query_operation_duration_seconds_sum[5m]) / rate(inferadb_query_operation_duration_seconds_count[5m])) * 1000)",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": { "Time": true },
              "renameByName": { "operation": "Operation", "Value": "Avg Duration (ms)" }
            }
          }
        ]
      },
      {
        "id": 4,
        "title": "Operation Throughput",
        "type": "graph",
        "gridPos": { "h": 10, "w": 12, "x": 0, "y": 18 },
        "targets": [
          {
            "expr": "sum by (operation) (rate(inferadb_query_operations_total[5m]))",
            "legendFormat": "{{operation}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "reqps", "label": "Operations/sec" },
          { "format": "short" }
        ],
        "legend": {
          "show": true,
          "alignAsTable": true,
          "rightSide": false,
          "avg": true,
          "max": true,
          "current": true
        }
      },
      {
        "id": 5,
        "title": "Latency Heatmap",
        "type": "heatmap",
        "gridPos": { "h": 10, "w": 12, "x": 12, "y": 18 },
        "targets": [
          {
            "expr": "sum(increase(inferadb_query_operation_duration_seconds_bucket[1m])) by (le)",
            "format": "heatmap",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "dataFormat": "tsbuckets",
        "heatmap": {},
        "yAxis": {
          "format": "s"
        }
      },
      {
        "id": 6,
        "title": "Check Operation Performance",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(inferadb_check_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.90, rate(inferadb_check_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "p90",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(inferadb_check_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "p99",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.999, rate(inferadb_check_duration_seconds_bucket[5m])) * 1000",
            "legendFormat": "p99.9",
            "refId": "D"
          }
        ],
        "yaxes": [
          { "format": "ms", "label": "Latency" },
          { "format": "short" }
        ]
      },
      {
        "id": 7,
        "title": "Evaluation Complexity",
        "type": "graph",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 28 },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(inferadb_evaluation_depth_bucket[5m]))",
            "legendFormat": "p50 Depth",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.90, rate(inferadb_evaluation_depth_bucket[5m]))",
            "legendFormat": "p90 Depth",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(inferadb_evaluation_depth_bucket[5m]))",
            "legendFormat": "p99 Depth",
            "refId": "C"
          }
        ],
        "yaxes": [
          { "format": "short", "label": "Evaluation Depth" },
          { "format": "short" }
        ]
      }
    ]
  }
}
